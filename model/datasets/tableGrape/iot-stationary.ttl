<DSD/iot-stationary> a qb:DataStructureDefinition;
  qb:component [qb:dimension bdg:plot; qb:componentAttachment qb:DataSet ];
  qb:component [qb:dimension bdg:dateTime ];
  qb:component [qb:attribute sosa:hasFeatureOfInterest ; qb:componentAttachment qb:MeasureProperty];
  qb:component [qb:measure   bdg:rainfall  ];
  qb:component [qb:measure   bdg:temp_air  ];
  qb:component [qb:measure   bdg:temp_system  ];
  qb:component [qb:measure   bdg:current_system  ];
  qb:component [qb:measure   bdg:radiation_solar  ];
  qb:component [qb:measure   bdg:moisture_soil_30cm  ];
  qb:component [qb:measure   bdg:moisture_soil_60cm  ];
  qb:component [qb:measure   bdg:humidity_air  ];
  qb:component [qb:measure   bdg:speed_wind  ];
  qb:component [qb:measure   bdg:direction_wind  ];
  qb:component [qb:measure   bdg:voltage_system  ].

<data/tableGrape/Palivou_IoT_2018> a qb:DataSet ;
