  qudt:hasMultiplier unit:Milli;
  qudt:conversionMultiplier 1.0e-3 ;
  qudt:conversionOffset "0.0"^^xsd:double ;
  qudt:hasQuantityKind quantitykind:ElectricConductivity ;
  qudt:isScalingOf unit:S-PER-M ;
