## Brix (refractive index)

<data/cosmetics> a qb:DataSet ;
    rdfs:label    "Measurement  for cosmetics pilot";
    qb:structure  <DSD/cosmetics> ;
    sosa:hasFeatureOfInterest <feature/VineLeaves> .

<DSD/cosmetics> a qb:DataStructureDefinition ;
    qb:component
    [qb:attribute sosa:hasFeatureOfInterest  ; qb:componentAttachment qb:DataSet],
    [qb:attribute sdmx-attribute:unitMeasure ; qb:componentAttachment qb:MeasureProperty],
    [qb:attribute bdg:assayMethod ; qb:componentAttachment qb:MeasureProperty ],
    [qb:attribute bdg:comparisonBaseline ; qb:componentAttachment qb:MeasureProperty ],
    [qb:dimension bdg:extractionMethod ],
    [qb:dimension bdg:date ],
    [qb:dimension bdg:grapeVariety ]
    [qb:measure   bdg:pH ],
    [qb:measure   bdg:refractiveIndex ],
    [qb:measure   bdg:yeastsAndMoulds ],
    [qb:measure   bdg:totalMicrobialCount ],
    [qb:measure   bdg:antioxidantActivityDPPHTrolox ],
    [qb:measure   bdg:antioxidantActivityABTSTrolox ],
    [qb:measure   bdg:TPCGallicAcid ],
    [qb:measure   bdg:TFCQuercetin ],





