 
<data/winemaking/PechRouge/sensoryAnalysis/2016> a qb:DataSet; 
  rdfs:label "Data about different wines which have been tested after fermentation at Pech Rouge in 2016"; 
  qb:structure <DSD/sensoryAnalysis/2016> . 
 
bdg:sensorialAnalysis a qb:AttributeProperty, qb:CodedProperty; 
  qb:codeList <sensorialAnalysis>; 
  rdfs:label "Tasting Flavor"; rdfs:comment "tasting flavor of the wine"; 
  rdfs:range bdg:SensorialAnalysis. 
 
# flavor list  
<sensorialAnalysis> a skos:ConceptScheme; 
  rdfs:label "Sensorial analysis codelist scheme"; 
  rdfs:seeAlso bdg:SensorialAnalysis . 
 
<sensorialAnalysis/sulfur> a skos:Concept, bdg:SensorialAnalysis; 
  skos:inScheme <sensorialAnalysis>; skos:topConceptOf <sensorialAnalysis>; 
  skos:notation "sulfur"; skos:prefLabel "taste of sulfur". 
  
<sensorialAnalysis-animal> a skos:Concept, bdg:SensorialAnalysis; 
  skos:inScheme <sensorialAnalysis>; skos:topConceptOf <sensorialAnalysis>; 
  skos:notation "animal"; skos:prefLabel "it smells like an animal". 
 
<DSD/sensoryAnalysis/2016> a qb:DataStructureDefinition; 
  qb:component [ qb:dimension bdg:statisticalReplication                                            ]; 
  qb:component [ qb:dimension bdg:wine                                                              ]; 
  qb:component [ qb:dimension bdg:date                                                              ]; 
  qb:component [ qb:dimension bdg:humanSensor                                                       ]; 
  qb:component [ qb:attribute bdg:sensorialAnalysis; qb:componentAttachment qb:Observation          ]; 
  qb:component [ qb:measure   bdg:tastingResult                                                     ]; 
  qb:component [ qb:attribute sosa:hasFeatureOfInterest; qb:componentAttachment qb:MeasureProperty  ]; 
  qb:component [ qb:attribute sdmx-attribute:unitMeasure; qb:componentAttachment qb:MeasureProperty ] . 

<INRA/judge/563> a bdg:humanSensor;
  rdfs:label "Judge 563";
  sosa:observes bdg:sensorialAnalysis .

<INRAjudge/1286> a bdg:humanSensor;
  rdfs:label "Judge 1286";
  sosa:observes bdg:sensorialAnalysis .

<INRAjudge/1180> a bdg:humanSensor;
  rdfs:label "Judge 1180";
  sosa:observes bdg:sensorialAnalysis .


