
<DSD/grapeMaturation/Brix> a qb:DataStructureDefinition;
  qb:component [qb:dimension bdg:vinestock          ];
  qb:component [qb:dimension bdg:date               ]; 
  qb:component [qb:measure   bdg:brix               ];
  qb:component [qb:attribute sosa:hasFeatureOfInterest ; qb:componentAttachment qb:DataSet         ];
  qb:component [qb:attribute sdmx-attribute:unitMeasure; qb:componentAttachment qb:MeasureProperty ].

<data/wineMaking/PechRouge/grapeMaturation/Brix> a qb:DataSet;
  rdfs:label "Data about grape maturation laboratory analysis of brix measures before harvest done at Pech Rouge";
  sosa:FeatureOfInterest afeo:Vinestock; # add a period attribute
  qb:structure <DSD/grapeMaturation/Brix> . 

