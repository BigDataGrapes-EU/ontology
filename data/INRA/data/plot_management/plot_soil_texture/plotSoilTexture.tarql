
CONSTRUCT {
  ?OBJECTCODE_URI a bdg:plotCluster ;
    rdfs:label ?OBJECTCODE ; 
    bdg:soilTexture ?SOILTYPE .
  
  ?BERRY rdf:type afeo:Berry .
} WHERE {

  BIND(REPLACE(REPLACE(REPLACE(?objectCode, "[^\\p{L}0-9]", "_"), "_+", "_"), "^_|_$", "") as ?OBJECTCODE)
  BIND(uri(CONCAT("INRA/estate/PechRouge/",REPLACE(?OBJECTCODE, " " ,""))) as ?OBJECTCODE_URI)

  BIND(REPLACE(REPLACE(REPLACE(?soilType, "[^\\p{L}0-9]", "_"), "_+", "_"), "^_|_$", "") as ?SOILTYPE)

}