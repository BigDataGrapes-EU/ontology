

<DSD/pressing> a qb:DataStructureDefinition;
  qb:component [qb:dimension bdg:experiment   ]; 
  qb:component [qb:dimension bdg:date         ]; 
  qb:component [qb:dimension bdg:pressingTank  ]; 
 # qb:component [qb:attribute bdg:operator   ]; # not constant
  qb:component [qb:measure   bdg:press_juice_volume     ];
  qb:component [qb:measure   bdg:run_juice_volume         ];
  qb:component [qb:measure   bdg:total_volume_pressing    ];
  qb:component [qb:attribute sosa:hasFeatureOfInterest ; qb:componentAttachment qb:DataSet         ];
  qb:component [qb:attribute sdmx-attribute:unitMeasure; qb:componentAttachment qb:MeasureProperty ].

<data/wineMaking/PechRouge/pressing/2017> a qb:DataSet;
  rdfs:label "Data about pressing operation done on Pech Rouge in 2017";
  sosa:FeatureOfInterest afeo:MustAF;
  bdg:date "2017-01-01"^^xsd:date ;
  qb:structure <DSD/pressing> . 

<data/wineMaking/PechRouge/pressing/2016> a qb:DataSet;
  rdfs:label "Data about pressing operation done on Pech Rouge in 2016";
  sosa:FeatureOfInterest afeo:MustAF;
  bdg:date "2016-01-01"^^xsd:date ;
  qb:structure <DSD/pressing> . 