<DSD/afterFermentation> a qb:DataStructureDefinition;
  qb:component [qb:dimension bdg:date                                    ];
  qb:component [qb:dimension afeo:AlcoholicFermentation                  ];                  
  qb:component [qb:measure   bdg:fermentation_temp_MIN                   ];
  qb:component [qb:measure   bdg:fermentation_temp_MAX                   ]; 
  qb:component [qb:measure   bdg:fermentation_density_MIN                ];
  qb:component [qb:measure   bdg:fermentation_density_MAX                ];
  qb:component [qb:measure   bdg:weight_alcoholic_fermentation           ];
  qb:component [qb:attribute sosa:hasFeatureOfInterest ; qb:componentAttachment qb:DataSet         ];
  qb:component [qb:attribute sdmx-attribute:unitMeasure; qb:componentAttachment qb:MeasureProperty ].

<data/wineMaking/PechRouge/AlcoholicFermentation> a qb:DataSet;
  rdfs:label "analysis during the process of fermentation";
  sosa:FeatureOfInterest afeo:AlcoholicFermentation;
  # bdg:date "2017-01-01"^^xsd:date ;
  qb:structure <DSD/afterFermentation> . 
