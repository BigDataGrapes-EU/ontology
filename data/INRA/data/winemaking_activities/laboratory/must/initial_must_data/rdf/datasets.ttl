<DSD/intialMust> a qb:DataStructureDefinition;
  qb:component [qb:dimension bdg:must               ]; 
  qb:component [qb:dimension bdg:date               ];
  qb:component [qb:dimension bdg:experiment         ]; # experiment 
  qb:component [qb:dimension bdg:laboratory         ]; # experiment 
  qb:component [qb:measure   bdg:totalSugar         ];
  qb:component [qb:measure   bdg:sugars             ];
  qb:component [qb:measure   bdg:probableAlcohol    ];
  qb:component [qb:measure   bdg:pH                 ];
  qb:component [qb:measure   bdg:totalAcidity       ];
  qb:component [qb:attribute sosa:hasFeatureOfInterest ; qb:componentAttachment qb:DataSet         ];
  qb:component [qb:attribute sdmx-attribute:unitMeasure; qb:componentAttachment qb:MeasureProperty ].

<data/wineMaking/PechRouge/intialMust/2017> a qb:DataSet;
  rdfs:label "Data about must initial done on Pech Rouge in 2017";
  sosa:FeatureOfInterest afeo:Must;
  bdg:date "2017-01-01"^^xsd:date ;
  qb:structure <DSD/intialMust> . 
