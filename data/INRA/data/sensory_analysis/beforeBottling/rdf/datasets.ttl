<DSD/sensoryAnalysisBeforeBottling> a qb:DataStructureDefinition;
  qb:component [ qb:dimension bdg:wine                  ];
  qb:component [ qb:dimension bdg:dateTime; qb:componentAttachment qb:DataSet ];
  qb:component [ qb:dimension bdg:sensoryFlavor         ];
  qb:component [ qb:measure   bdg:flavorJudgeRatio      ];
  qb:component [ qb:attribute sosa:hasFeatureOfInterest;  qb:componentAttachment  qb:DataSet        ];
  qb:component [ qb:attribute qudt:hasQuantityKind     ;  qb:componentAttachment  qb:MeasureProperty];
  qb:component [ qb:attribute sdmx-attribute:unitMeasure; qb:componentAttachment  qb:MeasureProperty];
.

<data/winemaking/sensoryAnalysis/2017> a qb:DataSet ;
  rdfs:label "Data about different wines, tasted after fermentation at Pech Rouge in 2017";
  qb:structure <DSD/sensoryAnalysisBeforeBottling> ;
  bdg:dateTime "2017-01-01T00:00:00"^^xsd:dateTime ;
.

