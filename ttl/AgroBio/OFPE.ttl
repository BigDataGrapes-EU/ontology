        rdfs:label          "begin date"@en ;
        rdfs:subPropertyOf  ofpe:date .

ofpe:code
        a                   owl:DatatypeProperty ;
        rdfs:label          "code"@en ;
        rdfs:subPropertyOf  owl:topDataProperty .

ofpe:comesFrom
        a           owl:ObjectProperty ;
        rdfs:label  "comes from"@en .

ofpe:contains
        a                   owl:TransitiveProperty , owl:ObjectProperty ;
        rdfs:label          "contains"@en ;
        rdfs:subPropertyOf  ofpe:temporalRelation ;
        owl:inverseOf       ofpe:during .

ofpe:contains_directly
        a                   owl:ObjectProperty ;
        rdfs:label          "contains directly"@en ;
