#################### OFPE classes

ofpe:Attribute
        a                owl:Class ;
        rdfs:label       "Object attribute"@en ;
        rdfs:subClassOf  owl:Thing .

ofpe:ByProduct
        a                owl:Class ;
        rdfs:label       "Co-produit"@fr , "By-Product"@en ;
        rdfs:subClassOf  ofpe:FinishedProduct .

ofpe:CalculationResult
        a                owl:Class ;
        rdfs:label       "Calculation result"@en ;
        rdfs:subClassOf  ofpe:ObservationResult .

ofpe:CoreProduct
        a                owl:Class ;
        rdfs:label       "Produit principal"@fr , "Core Product"@en ;
        rdfs:subClassOf  ofpe:FinishedProduct .

ofpe:ExpertJudgementResult
        a                owl:Class ;
        rdfs:label       "Expert judgement result"@en ;
        rdfs:subClassOf  ofpe:ObservationResult .

ofpe:FinishedProduct
        a                owl:Class ;
        rdfs:label       "Produit fini"@fr , "Finished Product"@en ;
        rdfs:subClassOf  ofpe:Product .

ofpe:FoodProcessing
        a                owl:Class ;
        rdfs:label       "Food Processing"@en ;
        rdfs:subClassOf  owl:Thing .

ofpe:InputProduct
        a                owl:Class ;
        rdfs:comment     "Input Product"@en ;
        rdfs:subClassOf  ofpe:Product .

ofpe:IntermediateProduct
        a                owl:Class ;
        rdfs:label       "Produit intermédiaire"@fr , "Intermediate Product"@en ;
        rdfs:subClassOf  ofpe:Product .

ofpe:MainOperation
        a                owl:Class ;
        rdfs:label       "Main Operation"@en ;
        rdfs:subClassOf  ofpe:Operation .

ofpe:MultipleObservations
        a                owl:Class ;
        rdfs:label       "Multiple observation" ;
        rdfs:subClassOf  ofpe:Observation ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      ofpe:hasMember ;
                           owl:someValuesFrom  ofpe:SingleObservation
                         ] .

ofpe:Observation
        a                owl:Class ;
        rdfs:label       "Observation"@en ;
        rdfs:subClassOf  owl:Thing ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  ofpe:ObservationMethod ;
                           owl:onProperty     ofpe:uses
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  ofpe:ObservationResult ;
                           owl:onProperty     ofpe:produces
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  ofpe:Product ;
                           owl:onProperty     ofpe:hasObservedObject
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  ofpe:Operation ;
                           owl:onProperty     ofpe:hasObservedObject
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  ofpe:Attribute ;
                           owl:onProperty     ofpe:hasObservedAttribute
                         ] .

ofpe:ObservationMethod
        a                owl:Class ;
        rdfs:label       "Observation Method"@en ;
        rdfs:subClassOf  owl:Thing .

ofpe:ObservationResult
        a                owl:Class ;
        rdfs:label       "Observation result"@en ;
        rdfs:subClassOf  owl:Thing ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  ofpe:Unit ;
                           owl:onProperty     ofpe:hasUnit
                         ] .

ofpe:Operation
        a                owl:Class ;
        rdfs:label       "Opération"@fr , "Operation"@en ;
        rdfs:subClassOf  owl:Thing ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange              xsd:anyURI ;
                           owl:onProperty               ofpe:endDate
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange              xsd:anyURI ;
                           owl:onProperty               ofpe:beginDate
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      ofpe:hasAttribute ;
                           owl:someValuesFrom  ofpe:OperationAttribute
                         ] .

ofpe:OperationAttribute
        a                 owl:Class ;
        rdfs:label        "Operation Attribute" ;
        rdfs:subClassOf   ofpe:Attribute ;
        owl:disjointWith  ofpe:ProductAttribute .

ofpe:Operator
        a                owl:Class ;
        rdfs:label       "Operator"@en , "Operateur"@fr ;
        rdfs:subClassOf  ofpe:Person .

ofpe:Organization
        a                owl:Class ;
        rdfs:label       "Organization"@en , "Organisme"@fr ;
        rdfs:subClassOf  owl:Thing ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  rdfs:Literal ;
                           owl:onProperty     ofpe:name
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  rdfs:Literal ;
                           owl:onProperty     ofpe:email
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  rdfs:Literal ;
                           owl:onProperty     ofpe:address
                         ] .

ofpe:Person
        a                owl:Class ;
        rdfs:label       "Person in charge"@en ;
        rdfs:subClassOf  owl:Thing ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  rdfs:Literal ;
                           owl:onProperty     ofpe:lastName
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  rdfs:Literal ;
                           owl:onProperty     ofpe:firstName
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  rdfs:Literal ;
                           owl:onProperty     ofpe:email
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  rdfs:Literal ;
                           owl:onProperty     ofpe:address
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  ofpe:Organization ;
                           owl:onProperty     ofpe:hasOrganization
                         ] .

ofpe:Plant
        a                owl:Class ;
        rdfs:label       "Plant"@en ;
        rdfs:subClassOf  owl:Thing .

ofpe:PostOperation
        a                owl:Class ;
        rdfs:label       "Post Operation"@en ;
        rdfs:subClassOf  ofpe:Operation .

ofpe:PreliminaryOperation
        a                owl:Class ;
        rdfs:label       "Preliminary Operation"@en ;
        rdfs:subClassOf  ofpe:Operation .

ofpe:Product
        a                owl:Class ;
        rdfs:label       "Produit"@fr , "Product"@en ;
        rdfs:subClassOf  owl:Thing ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      ofpe:hasAttribute ;
                           owl:someValuesFrom  ofpe:ProductAttribute
                         ] .

ofpe:ProductAttribute
        a                owl:Class ;
        rdfs:label       "Product Attribute" ;
        rdfs:subClassOf  ofpe:Attribute .

ofpe:SensorResult
        a                owl:Class ;
        rdfs:seeAlso     "Sensor Result"@en ;
        rdfs:subClassOf  ofpe:ObservationResult .

ofpe:ServiceProduct
        a                owl:Class ;
        rdfs:label       "Service Product"@en , "Intrant"@fr ;
        rdfs:subClassOf  ofpe:Product .

ofpe:SingleObservation
        a                owl:Class ;
        rdfs:label       "Single observation" ;
        rdfs:subClassOf  ofpe:Observation ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      ofpe:observationDate ;
                           owl:someValuesFrom  xsd:anyURI
                         ] .

ofpe:Unit
        a                owl:Class ;
        rdfs:label       "Unit of Measurement"@en ;
        rdfs:subClassOf  owl:Thing .

#################### OFPE properties

ofpe:address
        a                   owl:DatatypeProperty ;
        rdfs:label          "addresse"@en ;
        rdfs:subPropertyOf  owl:topDataProperty .

ofpe:after
        a                   owl:TransitiveProperty , owl:ObjectProperty ;
        rdfs:label          "after"@en ;
        rdfs:subPropertyOf  ofpe:temporalRelation ;
        owl:inverseOf       ofpe:before .

ofpe:after_directly
        a                   owl:ObjectProperty ;
        rdfs:label          "after directly"@en ;
        rdfs:subPropertyOf  ofpe:after ;
        owl:inverseOf       ofpe:before_directly .

ofpe:before
        a                   owl:TransitiveProperty , owl:ObjectProperty ;
        rdfs:label          "before"@en ;
