ofpe:date
        a                   owl:DatatypeProperty ;
        rdfs:label          "date"@en ;
        rdfs:subPropertyOf  ofpe:hasTimeValue .

ofpe:description
        a                   owl:DatatypeProperty ;
        rdfs:label          "description"@en ;
        rdfs:subPropertyOf  owl:topDataProperty .

ofpe:during
        a                   owl:TransitiveProperty , owl:ObjectProperty ;
        rdfs:label          "during"@en ;
        rdfs:subPropertyOf  ofpe:temporalRelation .

ofpe:email
        a                   owl:DatatypeProperty ;
        rdfs:label          "email"@en ;
        rdfs:subPropertyOf  owl:topDataProperty .

ofpe:endDate
        a                   owl:DatatypeProperty ;
        rdfs:label          "end date"@en ;
        rdfs:subPropertyOf  ofpe:date .

ofpe:equal
        a                   owl:ObjectProperty ;
        rdfs:label          "equal"@en ;
        rdfs:subPropertyOf  ofpe:temporalRelation .

ofpe:finishedBy
        a                   owl:ObjectProperty ;
        rdfs:label          "finished by"@en ;
        rdfs:subPropertyOf  ofpe:temporalRelation ;
        owl:inverseOf       ofpe:finishes .

ofpe:finishes
        a                   owl:ObjectProperty ;
        rdfs:label          "finishes"@en ;
        rdfs:subPropertyOf  ofpe:temporalRelation .

ofpe:firstName
        a                   owl:DatatypeProperty ;
        rdfs:label          "first name"@en ;
        rdfs:subPropertyOf  ofpe:name .

ofpe:gram
        a           ofpe:Unit , owl:NamedIndividual ;
        rdfs:label  "g" .

ofpe:gram_H2SO4_per_litre
        a           ofpe:Unit , owl:NamedIndividual ;
        rdfs:label  "g H2SO4/L" .

ofpe:gram_per_litre
        a           ofpe:Unit , owl:NamedIndividual ;
        rdfs:label  "g/L" .

ofpe:harvests
        a               owl:ObjectProperty ;
        owl:deprecated  "" .

ofpe:hasAttribute
        a            owl:ObjectProperty ;
        rdfs:domain  [ a            owl:Class ;
                       owl:unionOf  ( ofpe:Operation> <http://www.mistea.supagro.inra.fr/ontologies/2016/v4/ontofp#Product )
                     ] ;
        rdfs:label   "has attribute"@en ;
        rdfs:range   ofpe:Attribute .

ofpe:hasInput
        a              owl:TransitiveProperty , owl:ObjectProperty ;
        rdfs:domain    ofpe:Operation ;
        rdfs:label     "has input"@en ;
        rdfs:range     ofpe:Product ;
        owl:inverseOf  ofpe:isInputOf .

ofpe:hasInput_directly
        a                   owl:ObjectProperty ;
        rdfs:label          "has input directly"@en ;
        rdfs:subPropertyOf  ofpe:hasInput ;
        owl:inverseOf       ofpe:isInputOf_directly .

ofpe:hasMember
        a              owl:ObjectProperty ;
        rdfs:domain    ofpe:MultipleObservations ;
        rdfs:label     "is member"@en ;
        rdfs:range     ofpe:SingleObservation ;
        owl:inverseOf  ofpe:isMemberOf .

ofpe:hasMethod
        a            owl:ObjectProperty ;
        rdfs:domain  ofpe:Observation ;
        rdfs:label   "has method"@en ;
        rdfs:range   ofpe:ObservationMethod .

ofpe:hasObservationMethod
        a                   owl:ObjectProperty ;
        rdfs:domain         ofpe:Observation ;
        rdfs:label          "has observation method"@en ;
        rdfs:range          ofpe:ObservationMethod ;
        rdfs:subPropertyOf  owl:topObjectProperty ;
        owl:inverseOf       ofpe:isObservationMethodOf .

ofpe:hasObservationResult
        a              owl:ObjectProperty ;
        rdfs:domain    ofpe:Observation ;
        rdfs:label     "has observation result"@en ;
        rdfs:range     ofpe:ObservationResult ;
        owl:inverseOf  ofpe:isObservationResultOf .

ofpe:hasObservedAttribute
        a              owl:ObjectProperty ;
        rdfs:domain    ofpe:Observation ;
        rdfs:label     "has observed attribute"@en ;
        rdfs:range     ofpe:Attribute ;
        owl:inverseOf  ofpe:isObservedAttributeOf .

ofpe:hasObservedObject
        a              owl:ObjectProperty ;
        rdfs:domain    ofpe:Observation ;
        rdfs:label     "has observed object"@en ;
        rdfs:range     [ a            owl:Class ;
                         owl:unionOf  ( ofpe:Operation> <http://www.mistea.supagro.inra.fr/ontologies/2016/v4/ontofp#Product )
                       ] ;
        owl:inverseOf  ofpe:isObservedObjectOf .

ofpe:hasOrganization
        a              owl:ObjectProperty ;
        rdfs:domain    ofpe:Person ;
        rdfs:label     "has organization"@en ;
        rdfs:range     ofpe:Organization ;
        owl:inverseOf  ofpe:isOrganizationOf .

ofpe:hasOutput
        a              owl:TransitiveProperty , owl:ObjectProperty ;
        rdfs:domain    ofpe:Operation ;
        rdfs:label     "has output"@en ;
        rdfs:range     ofpe:Product ;
        owl:inverseOf  ofpe:isOutputOf .

ofpe:hasOutput_directly
        a                   owl:ObjectProperty ;
        rdfs:label          "has output directly"@en ;
        rdfs:subPropertyOf  ofpe:hasOutput ;
        owl:inverseOf       ofpe:isOutputOf_directly .

ofpe:hasPerson
        a              owl:ObjectProperty ;
        rdfs:label     "has Person"@en ;
        rdfs:range     ofpe:Person ;
        owl:inverseOf  ofpe:isPersonOf .

ofpe:hasReferenceValue
        a            owl:ObjectProperty ;
        rdfs:domain  ofpe:Observation ;
        rdfs:label   "has reference value"@en .

ofpe:hasTimeValue
        a                   owl:DatatypeProperty ;
        rdfs:label          "has time value"@en ;
        rdfs:subPropertyOf  owl:topDataProperty .

ofpe:hasUnit
        a              owl:FunctionalProperty , owl:ObjectProperty ;
        rdfs:domain    ofpe:ObservationResult ;
        rdfs:label     "has unit"@en ;
        rdfs:range     ofpe:Unit ;
        owl:inverseOf  ofpe:isUnitOf .

ofpe:hasValue
        a                   owl:DatatypeProperty ;
        rdfs:domain         ofpe:ObservationResult ;
        rdfs:label          "has value"@en ;
        rdfs:subPropertyOf  owl:topDataProperty .

ofpe:hour
        a                   owl:DatatypeProperty ;
        rdfs:label          "hour"@en ;
        rdfs:subPropertyOf  ofpe:hasTimeValue .

ofpe:isInputOf
        a            owl:TransitiveProperty , owl:ObjectProperty ;
        rdfs:domain  ofpe:Product ;
        rdfs:label   "is input of"@en ;
        rdfs:range   ofpe:Operation .

ofpe:isInputOf_directly
        a                   owl:ObjectProperty ;
        rdfs:label          "is input of directly"@en ;
        rdfs:subPropertyOf  ofpe:isInputOf .

ofpe:isMemberOf
        a            owl:ObjectProperty ;
        rdfs:domain  ofpe:SingleObservation ;
        rdfs:label   "is member of"@en ;
        rdfs:range   ofpe:MultipleObservations .

ofpe:isObservationMethodOf
        a            owl:ObjectProperty ;
        rdfs:domain  ofpe:ObservationMethod ;
        rdfs:label   "is observation method of"@en ;
        rdfs:range   ofpe:Observation .

ofpe:isObservationResultOf
        a            owl:ObjectProperty ;
        rdfs:domain  ofpe:ObservationResult ;
        rdfs:label   "is observation result of"@en ;
        rdfs:range   ofpe:Observation .

ofpe:isObservedAttributeOf
        a            owl:ObjectProperty ;
        rdfs:domain  ofpe:Attribute ;
        rdfs:label   "is observed attribut of"@en ;
        rdfs:range   ofpe:Observation .

ofpe:isObservedObjectOf
        a            owl:ObjectProperty ;
        rdfs:domain  [ a            owl:Class ;
                       owl:unionOf  ( ofpe:Operation> <http://www.mistea.supagro.inra.fr/ontologies/2016/v4/ontofp#Product )
                     ] ;
        rdfs:label   "is observed object of"@en ;
        rdfs:range   ofpe:Observation .

ofpe:isOrganizationOf
        a            owl:ObjectProperty ;
        rdfs:domain  ofpe:Organization ;
        rdfs:label   "is organization of"@en ;
        rdfs:range   ofpe:Person .

ofpe:isOutputOf
        a            owl:TransitiveProperty , owl:ObjectProperty ;
        rdfs:domain  ofpe:Product ;
        rdfs:label   "is output of"@en ;
        rdfs:range   ofpe:Operation .

ofpe:isOutputOf_directly
        a                   owl:ObjectProperty ;
        rdfs:label          "is output of directly"@en ;
        rdfs:subPropertyOf  ofpe:isOutputOf .

ofpe:isPersonOf
        a            owl:ObjectProperty ;
        rdfs:domain  ofpe:Person ;
        rdfs:label   "is Person In Charge of"@en .

ofpe:isUnitOf
        a            owl:FunctionalProperty , owl:ObjectProperty ;
        rdfs:domain  ofpe:Unit ;
        rdfs:label   "is unit of"@en ;
        rdfs:range   ofpe:ObservationResult .

ofpe:isUsedBy
        a              owl:TransitiveProperty , owl:ObjectProperty ;
        rdfs:label     "is used by"@en ;
        owl:inverseOf  ofpe:uses .

ofpe:isUsedBy_directly
        a                   owl:ObjectProperty ;
        rdfs:label          "is used by directly"@en ;
        rdfs:subPropertyOf  ofpe:isUsedBy ;
        owl:inverseOf       ofpe:uses_directly .

ofpe:kilogram
        a           ofpe:Unit , owl:NamedIndividual ;
        rdfs:label  "kg" .

ofpe:lastName
        a                   owl:DatatypeProperty ;
        rdfs:label          "last name"@en ;
        rdfs:subPropertyOf  ofpe:name .

ofpe:litre
        a           ofpe:Unit , owl:NamedIndividual ;
        rdfs:label  "L" .

ofpe:meets
        a                   owl:ObjectProperty ;
        rdfs:label          "meets"@en ;
        rdfs:subPropertyOf  ofpe:temporalRelation ;
        owl:inverseOf       ofpe:metBy .

ofpe:metBy
        a                   owl:ObjectProperty ;
        rdfs:label          "metBy"@en ;
        rdfs:subPropertyOf  ofpe:temporalRelation .

ofpe:microgram
        a           ofpe:Unit , owl:NamedIndividual ;
        rdfs:label  "µg" .

ofpe:microgram_eq
        a           ofpe:Unit , owl:NamedIndividual ;
        rdfs:label  "µg éq. DMS/L" .

ofpe:microgram_per_kilogram
        a           ofpe:Unit , owl:NamedIndividual ;
        rdfs:label  "µg/kg" .

ofpe:microgram_per_litre
        a           ofpe:Unit , owl:NamedIndividual ;
        rdfs:label  "µg/L" .

ofpe:miligram
        a           ofpe:Unit , owl:NamedIndividual ;
        rdfs:label  "mg" .

ofpe:miligram_per_kilogram
        a           ofpe:Unit , owl:NamedIndividual ;
        rdfs:label  "mg/kg" .

ofpe:miligram_per_litre
        a           ofpe:Unit , owl:NamedIndividual ;
        rdfs:label  "mg/L" .

ofpe:name
        a                   owl:DatatypeProperty ;
        rdfs:label          "name"@en ;
        rdfs:subPropertyOf  owl:topDataProperty .

ofpe:observationDate
        a                   owl:DatatypeProperty ;
        rdfs:domain         ofpe:SingleObservation ;
        rdfs:label          "observation date"@en ;
        rdfs:subPropertyOf  ofpe:date .

ofpe:overlappedBy
        a                   owl:ObjectProperty ;
        rdfs:label          "overlapped by"@en ;
        rdfs:subPropertyOf  ofpe:temporalRelation ;
        owl:inverseOf       ofpe:overlaps .

ofpe:overlaps
        a                   owl:ObjectProperty ;
        rdfs:label          "overlaps"@en ;
        rdfs:subPropertyOf  ofpe:temporalRelation .

ofpe:percent_volume
        a           ofpe:Unit , owl:NamedIndividual ;
        rdfs:label  "%vol" .

ofpe:produces
        a            owl:ObjectProperty ;
        rdfs:domain  ofpe:Observation ;
        rdfs:label   "produces"@en ;
        rdfs:range   ofpe:ObservationResult .

ofpe:producesAfter
        a              owl:TransitiveProperty , owl:ObjectProperty ;
        rdfs:domain    ofpe:Product ;
        rdfs:label     "produces after"@en ;
