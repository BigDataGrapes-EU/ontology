qudt:QuantityValue
      rdf:type owl:Class ;
      rdfs:label "Quantity value"^^xsd:string ;
      rdfs:subClassOf owl:Thing ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom qudt:Unit ;
                owl:onProperty qudt:unit
              ] ;
