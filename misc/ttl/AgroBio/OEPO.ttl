#################### OEPO Classes


oepo:AccessionName
        a                 owl:Class ;
        rdfs:isDefinedBy  "skos:closeMatch http://www.cropontology.org/rdf/CO_020:0000011" ;
        rdfs:label        "Nom de l'accession"@fr , "Accession name"@en ;
        rdfs:subClassOf   oepo:GermplasmDescriptor .

oepo:AccessionNumber
        a                 owl:Class ;
        rdfs:isDefinedBy  "skos:closeMatch http://www.cropontology.org/rdf/CO_020:0000089" ;
        rdfs:label        "Numéro d'accession"@fr , "Accession number"@en ;
        rdfs:subClassOf   oepo:GermplasmDescriptor .

oepo:ActiveSpectroradiometer
        a                owl:Class ;
        rdfs:comment     "Active instruments provide their own energy (electromagnetic radiation) to illuminate the object or scene they observe. They send a pulse of energy from the sensor to the object and then receive the radiation that is reflected or backscattered from that object. Scientists use many different types of active remote sensors."@en ;
        rdfs:label       "Active spectroradiometer"@en ;
        rdfs:subClassOf  oepo:Spectroradiometer ;
        dc:source        "https://earthobservatory.nasa.gov/Features/RemoteSensing/remote_08.php"@en .

oepo:Actuator
        a                owl:Class ;
        rdfs:comment     "Devices that usually are responsible for moving and controlling a mechanism or system, for example by opening a valve or opening shadows in a greenhouse."@en ;
        rdfs:label       "Actuator"@en , "Actionneur"@fr ;
        rdfs:subClassOf  oepo:Device .

oepo:AirConditioning
        a                owl:Class ;
        rdfs:label       "Hot or cold air conditioning system"@en , "Aerotherme en chaud ou froid"@fr ;
        rdfs:subClassOf  oepo:Actuator .

oepo:AirMixer
        a                owl:Class ;
        rdfs:label       "Brassage air"@fr , "Air Mixer"@en ;
        rdfs:subClassOf  oepo:Actuator .

oepo:Area
        a                owl:Class ;
        rdfs:comment     "Zone au sens large.\nPeut définir une zone au sens physique ou théorique"@fr ;
        rdfs:label       "Zone"@fr , "Area"@en ;
        rdfs:subClassOf  oepo:Equipement .

oepo:CO2Sensor
        a                owl:Class ;
        rdfs:label       "CO2 sensor"@en ;
        rdfs:subClassOf  oepo:SensingDevice .

oepo:Camera
        a                owl:Class ;
        rdfs:label       "Caméra"@fr , "Camera"@en ;
        rdfs:subClassOf  oepo:SensingDevice .

oepo:CapacitanceSensor
        a                 owl:Class ;
        rdfs:comment      "certain materials interact with water vapour and undergo a change in capacitance, in relation to ambient humidity." ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.oclc.org/NET/ssnx/meteo/aws#CapacitanceSensor" ;
        rdfs:label        "Capacitance sensor"@en ;
        rdfs:subClassOf   oepo:HumiditySensor .

oepo:CapacitiveThinFilmPolymer
        a                 owl:Class ;
        rdfs:comment      "Certain materials interact with water vapour and undergo a change in capacitance, in relation to ambient humidity." ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.oclc.org/NET/ssnx/meteo/aws#CapacitiveThinFilmPolymer" ;
        rdfs:label        "Capacitive - thin film polymer"@en ;
        rdfs:subClassOf   oepo:CapacitanceSensor .

oepo:CarSupport
        a                owl:Class ;
        rdfs:label       "Chariot"@fr , "Car support"@en ;
        rdfs:subClassOf  oepo:Vector .

oepo:Compartment
        a                owl:Class ;
        rdfs:label       "Compartment"@en , "Compartiment"@fr ;
        rdfs:subClassOf  oepo:ProtectiveStructure .

oepo:ConveyorBelt
        a                owl:Class ;
        rdfs:label       "Convoyeur"@fr , "Conveyor belt"@en ;
        rdfs:subClassOf  oepo:Vector .

oepo:CupAnemometer
        a                 owl:Class ;
        rdfs:comment      "The angular velocity of the cup is directly proportional to the component of the wind speed parallel to the axis of rotation." ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.oclc.org/NET/ssnx/meteo/aws#CupAnemometer" ;
        rdfs:label        "Cup anemometer"@en ;
        rdfs:subClassOf   oepo:WindSensor .

oepo:DataFile
        a                owl:Class ;
        rdfs:label       "Fichier de données"@fr , "Data file"@en ;
        rdfs:subClassOf  oepo:Document .

oepo:DataSet
        a                 owl:Class ;
        rdfs:comment      "A MIAPPE-compliant dataset, derived from a study or investigation"@en ;
        rdfs:isDefinedBy  "skos:exactMatch https://raw.githubusercontent.com/MIAPPE/MIAPPE-ontology/master/PPEO.owl#PPEO_000003"@en ;
        rdfs:label        "Data set"@en ;
        rdfs:subClassOf   oepo:Document .

oepo:Device
        a                owl:Class ;
        rdfs:label       "Device"@en ;
        rdfs:subClassOf  oepo:Equipement .

oepo:Document
        a                owl:Class ;
        rdfs:label       "Document"@fr , "Document"@en ;
        rdfs:subClassOf  owl:Thing .

oepo:Dome
        a                owl:Class ;
        rdfs:label       "Dome"@en , "Chapelle"@fr ;
        rdfs:subClassOf  oepo:ProtectiveStructure .

oepo:EarInflorescence
        a                 owl:Class ;
        rdfs:comment      "A pistillate inflorescence  (PO:0025598) in Zea species that is highly compacted and bears the ear spikelets (PO:0006320) on a lateral inflorescence axis   (PO:0020122). [database_cross_reference: MaizeGDB_curator:Leszek_Vincent][database_cross_reference: POC:Laurel_Cooper]"@en ;
        rdfs:isDefinedBy  "skos:exactMatch http://www.ontobee.org/ontology/rdf/PO?iri=http://purl.obolibrary.org/obo/PO_0020136" ;
        rdfs:label        "Epi"@fr , "Ear inflorescence"@en ;
        rdfs:subClassOf   oepo:ScientificObject .

oepo:ElectricalResistanceBlock
        a                 owl:Class ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.oclc.org/NET/ssnx/meteo/aws#ElectricalResistanceBlock" ;
        rdfs:label        "Electrical resistance block"@en ;
        rdfs:subClassOf   oepo:HumiditySensor .

oepo:ElectricalResistanceHumiditySensor
        a                 owl:Class ;
        rdfs:comment      "Certain materials interact with water vapour and undergo a change in electrical resistance, in relation to ambient humidity." ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.oclc.org/NET/ssnx/meteo/aws#ElectricalResistanceHumiditySensor" ;
        rdfs:label        "Electrical resistance humidity sensor"@en ;
        rdfs:subClassOf   oepo:HumiditySensor .

oepo:ElectricalResistanceThermometer
        a                 owl:Class ;
        rdfs:comment      "For small temperature changes, the increase in resistance of pure metals is proportional to the change in temperature. Often made in Platinum." ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.oclc.org/NET/ssnx/meteo/aws#ElectricalResistanceThermometer" ;
        rdfs:label        "Electrical resistance thermometer"@en ;
        rdfs:subClassOf   oepo:TemperatureSensor .

oepo:EnvironmentFeature
        a                 owl:Class ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.org/ppeo/PPEO.owl#PPEO_000010" ;
        rdfs:label        "Environment feature"@en ;
        rdfs:subClassOf   oepo:Quality .

oepo:Equipement
        a                owl:Class ;
        rdfs:comment     "An INSTALLATION is the elementary level for data acquisition in a specific type of experiments. It stands for other frequently used terms such as ‘platform’, ‘facility’ or others."@en ;
        rdfs:label       "Equipement"@fr , "Equipement"@en ;
        rdfs:subClassOf  oepo:ExperimentalCondition .

oepo:EuropeanInfrastructure
        a                owl:Class ;
        rdfs:comment     "The ESFRI listed project EMPHASIS aims at a synergistic development and long-term operation of plant phenotyping infrastructure in Europe (https://emphasis.plant-phenotyping.eu/) by developing tools and methods for multi-scale phenotyping, allowing one to analyze genotype performance and trait diversity in current and future European environmental conditions."@en ;
        rdfs:label       "European Infrastructure"@en ;
        rdfs:subClassOf  oepo:Infrastructure .

oepo:Experiment
        a                 owl:Class ;
        rdfs:comment      "An EXPERIMENT is a planned activity carried out by a given set of persons on a given set of observation unit, like plants or microplots (see §2.1), involving a protocol, methods and a resulting dataset."@en ;
        rdfs:isDefinedBy  "sko:exactMatch <brapi/v1/studies>" , "EMPHASIS ontology" ;
        rdfs:label        "Expérimentation"@fr , "Experiment"@en ;
        rdfs:subClassOf   owl:Thing .

oepo:ExperimentalCondition
        a                 owl:Class ;
        rdfs:comment      "A set of environmental conditions, infrastructure and supporting equipment or resources necessary to conduct a given experiment and in which plants, plots or group of plants are raised"@en ;
        rdfs:isDefinedBy  "skos:closeMatch http://www.ontobee.org/ontology/rdf/PECO?iri=http://purl.obolibrary.org/obo/PECO_0007359"@en , "skos:closeMatch http://www.cropontology.org/rdf/CO_715:0000005" ;
        rdfs:label        "Experimental condition"@en , "Context expérimental"@fr ;
        rdfs:subClassOf   owl:Thing .

oepo:Fan
        a                owl:Class ;
        rdfs:label       "Ventilateur"@fr , "Fan"@en ;
        rdfs:subClassOf  oepo:Actuator .

oepo:FiberGlassBlock
        a                 owl:Class ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.oclc.org/NET/ssnx/meteo/aws#FiberGlassBlock" ;
        rdfs:label        "Fiber-glass block"@en ;
        rdfs:subClassOf   oepo:ElectricalResistanceBlock .

oepo:Field
        a                owl:Class ;
        rdfs:label       "Field"@en , "Champ"@fr ;
        rdfs:subClassOf  oepo:Area .

oepo:FieldExperiment
        a                owl:Class ;
        rdfs:label       "Field experiment"@en ;
        rdfs:subClassOf  oepo:Experiment .

oepo:FieldRobot
        a                owl:Class ;
        rdfs:label       "Phenomobile"@fr , "Field robot"@en ;
        rdfs:subClassOf  oepo:Vector .

oepo:Gantry
        a                owl:Class ;
        rdfs:label       "Portique"@fr , "Gantry"@en ;
        rdfs:subClassOf  oepo:Vector .

oepo:Genus
        a                 owl:Class ;
        rdfs:isDefinedBy  "skos:exactMatch http://www.cropontology.org/rdf/CO_020:0000005" ;
        rdfs:label        "Genus"@en , "Genre"@fr ;
        rdfs:subClassOf   oepo:GermplasmDescriptor .

oepo:GermplasmDescriptor
        a                 owl:Class ;
        rdfs:isDefinedBy  "skos:closeMatch http://www.cropontology.org/rdf/CO_020:0000000" ;
        rdfs:label        "Germplasm descriptor information"@en ;
        rdfs:subClassOf   owl:Thing .

oepo:Greenhouse
        a                owl:Class ;
        rdfs:label       "Serre"@fr , "Greenhouse"@en ;
        rdfs:subClassOf  oepo:ProtectiveStructure .

oepo:GreenhouseExperiment
        a                owl:Class ;
        rdfs:label       "Greenhouse experiment"@en ;
        rdfs:subClassOf  oepo:Experiment .

oepo:GrippingArm
        a                owl:Class ;
        rdfs:label       "Gripping arm"@en , "Bras de préhension"@fr ;
        rdfs:subClassOf  oepo:Vector .

oepo:GrowthChamber
        a                owl:Class ;
        rdfs:label       "Growth chamber"@en , "Chambre de culture"@fr ;
        rdfs:subClassOf  oepo:ProtectiveStructure .

oepo:GrowthChamberExperiment
        a                owl:Class ;
        rdfs:label       "Growth chamber experiment"@en ;
        rdfs:subClassOf  oepo:Experiment .

oepo:GypsumBlock
        a                 owl:Class ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.oclc.org/NET/ssnx/meteo/aws#GypsumBlock" ;
        rdfs:label        "Gypsum block"@en ;
        rdfs:subClassOf   oepo:ElectricalResistanceBlock .

oepo:HemisphericalCamera
        a                owl:Class ;
        rdfs:label       "Hemispherical camera"@en , "Caméra hémisphérique"@fr ;
        rdfs:subClassOf  oepo:Camera .

oepo:HorizontalScreen
        a                owl:Class ;
        rdfs:label       "Ouvrant horizontal"@fr , "Horizontal screen"@en ;
        rdfs:subClassOf  oepo:Actuator .

oepo:HumiditySensor
        a                 owl:Class ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.oclc.org/NET/ssnx/meteo/aws#HumiditySensor" ;
        rdfs:label        "Humidity sensor"@en , "Capteur d'humidité"@fr ;
        rdfs:subClassOf   oepo:SensingDevice .

oepo:HyperspectralCamera
        a                owl:Class ;
        rdfs:label       "Hyperspectral camera"@en , "Caméra hyperspectrale"@fr ;
        rdfs:subClassOf  oepo:Camera .

oepo:ImagingCabin
        a                owl:Class ;
        rdfs:label       "Imaging cabin"@en ;
        rdfs:subClassOf  oepo:Actuator .

oepo:Infrastructure
        a                owl:Class ;
        rdfs:label       "Infrastructure"@fr , "Infrastructure"@en ;
        rdfs:subClassOf  oepo:ExperimentalCondition .

oepo:Installation
        a                owl:Class ;
        rdfs:comment     "An INSTALLATION is the elementary level for data acquisition in a specific type of experiments. It stands for other frequently used terms such as ‘platform’, ‘facility’ or others."@en ;
        rdfs:label       "Installation"@fr , "Installation"@en ;
        rdfs:subClassOf  oepo:Infrastructure .

oepo:IrrigationSystem
        a                owl:Class ;
        rdfs:label       "Système d'irrigation"@fr , "Irrigation system"@en ;
        rdfs:subClassOf  oepo:Actuator .

oepo:LabBook
        a                owl:Class ;
        rdfs:label       "Lab book"@en , "Cahier de laboratoire"@fr ;
        rdfs:subClassOf  oepo:Document .

oepo:Leaf
        a                 owl:Class ;
        rdfs:comment      "A phyllome vascular system (PO:0025206) that includes the totality of the portions of  vascular tissue in their specific arrangement in a vascular leaf  (PO:0009025). [database_cross_reference:  POC:Ramona_Walls]"@en ;
        rdfs:isDefinedBy  "skos:closeMatch http://www.ontobee.org/ontology/rdf/PO?iri=http://purl.obolibrary.org/obo/PO_0000036" ;
        rdfs:label        "Leaf"@en , "Feuille"@fr ;
        rdfs:subClassOf   oepo:ScientificObject .

oepo:LevelMeasurementRainGauge
        a                 owl:Class ;
        rdfs:comment      "Measuring the water level in a tube" ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.oclc.org/NET/ssnx/meteo/aws#LevelMeasurementRainGauge" ;
        rdfs:label        "Level measurement rain gauge"@en ;
        rdfs:subClassOf   oepo:PrecipitationSensor .

oepo:Lightning
        a                owl:Class ;
        rdfs:label       "Lightning"@en , "Eclairage"@fr ;
        rdfs:subClassOf  oepo:Actuator .

oepo:LocalInfrastructure
        a                owl:Class ;
        rdfs:comment     "A LOCAL INFRASTRUCTURE is a group of installations located in one site depending on one institution (or more), which share governance committees, a common (or at least highly interoperable) information system, common principles for cost calculation and pricing and a common tool for user access."@en ;
        rdfs:label       "Local infrastructure"@en , "Infrastructure locale"@fr ;
        rdfs:subClassOf  oepo:Infrastructure .

oepo:Method
        a                 owl:Class ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.org/ppeo/PPEO.owl#PPEO_000011" ;
        rdfs:label        "Method"@en ;
        rdfs:subClassOf   owl:Thing .

oepo:Multimedia
        a                owl:Class ;
        rdfs:label       "Multimedia"@fr , "Multimedia"@en ;
        rdfs:subClassOf  oepo:Document .

oepo:NationalInfrastructure
        a                owl:Class ;
        rdfs:comment     "A NATIONAL INFRASTRUCTURE is an organized group of local infrastructures with governance recognised by national authorities (e.g. respective ministry or national structures)."@en ;
        rdfs:label       "National infrastructure"@en , "Infrastructure nationale"@fr ;
        rdfs:subClassOf  oepo:Infrastructure .

oepo:NetRadiometerOrNetPyrradiometerOrNetExchangeRadiometerOrBalancemeter
        a                 owl:Class ;
        rdfs:comment      "Net all-wave radiation (net flux of downward and upward total radiation i.e. solar, terrestrial and atmospheric)" ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.oclc.org/NET/ssnx/meteo/aws#NetRadiometerOrNetPyrradiometerOrNetExchangeRadiometerOrBalancemeter" ;
        rdfs:label        "Net radiometer or net pyrradiometer or net exchange radiometer or balancemeter"@en ;
        rdfs:subClassOf   oepo:RadiationSensor .

oepo:NylonUnits
        a                 owl:Class ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.oclc.org/NET/ssnx/meteo/aws#NylonUnits" ;
        rdfs:label        "Nylon units"@en ;
        rdfs:subClassOf   oepo:ElectricalResistanceBlock .

oepo:Observation
        a                 owl:Class ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.org/ppeo/PPEO.owl#PPEO_000006" ;
        rdfs:label        "Observation"@en ;
        rdfs:subClassOf   owl:Thing ;
        rdfs:subClassOf   [ a                         owl:Restriction ;
                            owl:onClass               oepo:ObservationVariable ;
                            owl:onProperty            oepo:hasVariable ;
                            owl:qualifiedCardinality  "1"^^xsd:nonNegativeInteger
                          ] .

oepo:ObservationVariable
        a                 owl:Class ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.org/ppeo/PPEO.owl#PPEO_000007" ;
        rdfs:label        "Observation variable"@en ;
        rdfs:subClassOf   owl:Thing ;
        rdfs:subClassOf   [ a                         owl:Restriction ;
                            owl:onClass               oepo:Unit ;
                            owl:onProperty            oepo:hasUnit ;
                            owl:qualifiedCardinality  "1"^^xsd:nonNegativeInteger
                          ] ;
        rdfs:subClassOf   [ a                         owl:Restriction ;
                            owl:onClass               oepo:Quality ;
                            owl:onProperty            oepo:hasQuality ;
                            owl:qualifiedCardinality  "1"^^xsd:nonNegativeInteger
                          ] ;
        rdfs:subClassOf   [ a                         owl:Restriction ;
                            owl:onClass               oepo:Method ;
                            owl:onProperty            oepo:hasMethod ;
                            owl:qualifiedCardinality  "1"^^xsd:nonNegativeInteger
                          ] .

oepo:PassiveSpectroradiometer
        a                owl:Class ;
        rdfs:comment     "Passive instruments detect natural energy that is reflected or emitted from the observed scene. Passive instruments sense only radiation emitted by the object being viewed or reflected by the object from a source other than the instrument. Reflected sunlight is the most common external source of radiation sensed by passive instruments. Scientists use a variety of passive remote sensors."@en ;
        rdfs:label       "Passive spectroradiometer"@en ;
        rdfs:subClassOf  oepo:Spectroradiometer ;
        dc:source        "https://earthobservatory.nasa.gov/Features/RemoteSensing/remote_08.php"@en .

oepo:Plant
        a                 owl:Class ;
        rdfs:comment      "the continuous organism originating from a single seed, callus, rhizome or any other propagation mean"@en ;
        rdfs:isDefinedBy  "skos:closeMatch http://www.ontobee.org/ontology/rdf/PO?iri=http://purl.obolibrary.org/obo/PO_0000003" ;
        rdfs:label        "Plante"@fr , "Plant"@en ;
        rdfs:subClassOf   oepo:ScientificObject .

oepo:PlantGroup
        a                owl:Class ;
        rdfs:label       "Plant group"@en , "Groupe de plantes"@fr ;
        rdfs:subClassOf  oepo:ScientificObject .

oepo:Plot
        a                owl:Class ;
        rdfs:comment     "A community of plants located on a defined area of a field."@en ;
        rdfs:label       "Plot"@en , "Parcelle"@fr ;
        rdfs:subClassOf  oepo:ScientificObject .

oepo:Pot
        a                owl:Class ;
        rdfs:comment     "Pot de la plante"@fr ;
        rdfs:label       "Pot"@fr , "Pot"@en ;
        rdfs:subClassOf  oepo:Vector .

oepo:PrecipitationSensor
        a                 owl:Class ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.oclc.org/NET/ssnx/meteo/aws#PrecipitationSensor" ;
        rdfs:label        "Precipitation sensor"@en , "Capteur de précipitation"@fr ;
        rdfs:subClassOf   oepo:SensingDevice .

oepo:Project
        a                owl:Class ;
        rdfs:comment     "PROJECT (e.g. EU or national projects) involves partners that are in charge of experiments performed in one or more local infrastructures."@en ;
        rdfs:label       "Projet"@fr , "Project"@en ;
        rdfs:subClassOf  oepo:ExperimentalCondition .

oepo:PropellerAnemometer
        a                 owl:Class ;
        rdfs:comment      "The angular velocity of the propeller rotor is directly proportional to the component of the wind speed parallel to the axis of rotation."@en ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.oclc.org/NET/ssnx/meteo/aws#PropellerAnemometer" ;
        rdfs:label        "Propeller anemometer"@en ;
        rdfs:subClassOf   oepo:WindSensor .

oepo:ProtectiveStructure
        a                owl:Class ;
        rdfs:label       "Structure protectrice"@fr , "Protective structure"@en ;
        rdfs:subClassOf  oepo:Equipement .

oepo:Provider
        a                owl:Class ;
        rdfs:comment     "An organized group of people who perform experiments, usually together with users, handle equipment and uses a dedicated set of pipelines and procedures for the analysis of the data originating from the installation"@en ;
        rdfs:label       "Provider"@en ;
        rdfs:subClassOf  owl:Thing .

oepo:Pyranometer
        a                 owl:Class ;
        rdfs:comment      "Total solar irradiance on a horizontal surface" ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.oclc.org/NET/ssnx/meteo/aws#Pyranometer" ;
        rdfs:label        "Pyranometer"@en ;
        rdfs:subClassOf   oepo:RadiationSensor .

oepo:PyranometerWithShadeRing
        a                 owl:Class ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.oclc.org/NET/ssnx/meteo/aws#PyranometerWithShadeRing" ;
        rdfs:label        "Pyranometer with shade ring"@en ;
        rdfs:subClassOf   oepo:RadiationSensor .

oepo:Quality
        a                 owl:Class ;
        rdfs:isDefinedBy  "skos:exactMatch http://purl.org/ppeo/PPEO.owl#PPEO_000008" ;
        rdfs:label        "Quality"@en ;
